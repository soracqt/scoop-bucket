{
    "version": "main",
    "description": "Roboto Mono is a monospaced addition to the Roboto type family. ",
    "homepage": "https://fonts.google.com/specimen/Roboto+Mono",
    "license": "Apache License Version 2.0",
    "url": "https://github.com/googlefonts/RobotoMono/archive/refs/heads/main.zip",
    "pre_install": [
        "#Access denied error running: Remove-Item \"$env:windir/fonts/NotoSansJP*.otf\" -recurse"
    ],
    "post_install": [
        "$fonts = (New-Object -ComObject Shell.Application).Namespace(0x14)",
        "dir \"$dir/RobotoMono-main/*.ttf\" | %{ $fonts.CopyHere($_.fullname) }"
    ],
    "autoupdate": {
        "url": "https://github.com/googlefonts/RobotoMono/archive/refs/heads/main.zip"
    }
}
